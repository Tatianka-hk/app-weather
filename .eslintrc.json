{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "indent": ["warn", 2],
    "linebreak-style": ["error", "unix"],
    "no-console": ["warn"],
    "no-unused-vars": ["warn"],
    "vue/no-parsing-error": ["warn"],
    "no-undef":["warn"]
  }
}